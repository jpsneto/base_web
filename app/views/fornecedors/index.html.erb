<p id="notice"><%= notice %></p>

<h1>Listing Fornecedors</h1>

<table>
  <thead>
    <tr>
      <th>Nome fantasia</th>
      <th>Contato</th>
      <th>Email</th>
      <th>Logradouro</th>
      <th>Bairro</th>
      <th>Cidade</th>
      <th>Estado</th>
      <th>Cep</th>
      <th>Telefone</th>
      <th>Fax</th>
      <th>Cnpj</th>
      <th>Inscricao estadual</th>
      <th>Refenrecias bancarias</th>
      <th>Observacoes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fornecedors.each do |fornecedor| %>
      <tr>
        <td><%= fornecedor.nome_fantasia %></td>
        <td><%= fornecedor.contato %></td>
        <td><%= fornecedor.email %></td>
        <td><%= fornecedor.logradouro %></td>
        <td><%= fornecedor.bairro %></td>
        <td><%= fornecedor.cidade %></td>
        <td><%= fornecedor.estado %></td>
        <td><%= fornecedor.cep %></td>
        <td><%= fornecedor.telefone %></td>
        <td><%= fornecedor.fax %></td>
        <td><%= fornecedor.cnpj %></td>
        <td><%= fornecedor.inscricao_estadual %></td>
        <td><%= fornecedor.refenrecias_bancarias %></td>
        <td><%= fornecedor.observacoes %></td>
        <td><%= link_to 'Show', fornecedor %></td>
        <td><%= link_to 'Edit', edit_fornecedor_path(fornecedor) %></td>
        <td><%= link_to 'Destroy', fornecedor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fornecedor', new_fornecedor_path %>
